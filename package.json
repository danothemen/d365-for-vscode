{
	"name": "d365-for-vscode",
	"displayName": "D365 For VSCode",
	"description": "Keep your local web resources in sync with your dynamics CE instance",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.d365signin",
				"title": "D365: Sign In"
			},
			{
				"command": "extension.d365setup",
				"title": "D365: Set Up"
			},
			{
				"command": "extension.d365map",
